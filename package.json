{
  "name": "expense-tracker-api",
  "version": "1.0.0",
  "description": "A simple RESTful API for an expense tracker application.",
  "keywords": [
    "express",
    "knex",
    "expense-tracker"
  ],
  "author": "Pham Bao Huy",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "migrate": "npx knex migrate:latest --knexfile src/db/knexfile.js",
    "seed": "npx knex seed:run --knexfile src/db/knexfile.js",
    "dev": "node --env-file=.env --watch src/server.js"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "zod": "^4.2.1"
  }
}
